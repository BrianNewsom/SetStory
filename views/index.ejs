<html>
<head>

	<script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/bower_components/d3/d3.min.js"></script>
    <script src="/bower_components/wow/dist/wow.min.js"></script>

    <script src="/bower_components/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="/bower_components/waypoints/lib/shortcuts/infinite.min.js"></script>


	<script type='text/javascript' src='/bower_components/angularjs/angular.min.js'></script>
	<script type='text/javascript' src='/bower_components/ngInfiniteScroll/build/ng-infinite-scroll.min.js'></script>


    <link rel="stylesheet" type="text/css" href="/bower_components/wow/css/libs/animate.css"/>
	<link rel="stylesheet" type="text/css" href="stylesheets/style.css">

</head>
<body>

	<div class="buffer-big"></div>
	<div class="search-container">
		<h1 class="wow fadeIn" style="margin-left:1vw">How Has Your Favorite Artist Changed Over Time?</h1>	
		<br>
		<input class="search-bar" id="search-bar" type="text" autocomplete="off" placeholder="search artists...">
	</div>
	<div class="buffer-big"></div>	

	<div ng-app='myApp' ng-controller='DemoController'>
	  <div infinite-scroll='loadMore()' infinite-scroll-distance='2'>
	    <div ng-repeat='concert in concerts'> 
	    <img ng-repeat='image in concert.tracks' ng-src='http://placehold.it/225x250&text={{$index}}'>
	  </div>
	</div>
    <script src="/scripts/services.js"></script>
    <script src="/scripts/app.js"></script>

    <script src="/scripts/ng/main.js"></script>
</html>
</html>
